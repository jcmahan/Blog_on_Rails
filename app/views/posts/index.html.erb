<h1>James' Blog App</h1>

<a href="#">New Post</a>

<table>
    <tbody>
    <tr>
    <th>Post Title</th>
    <th>upvotes</th>
    <th>downvotes</th>
    <th>Date Published</th>
    </tr>

 <% @posts.each do |post|%> 
    <tr>
        <td><%= post.title %></td>
        <td><%= post.upvotes %></td>
        <td><%= post.downvotes %></td>
        <td><%= post.created_at %></td>
    </tr>
    <%end%>
    </tbody>
</table>